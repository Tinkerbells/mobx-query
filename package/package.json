{
  "name": "@tinkerbells/mobx-query",
  "version": "1.18.2",
  "browser": "./src/index.ts",
  "main": "./src/index.ts",
  "scripts": {
    "build": "npm run clean && npm run bundle && npm run build:package",
    "bundle": "tsdown --config tsdown.config.mts",
    "clean": "rm -rf lib",
    "build:package": "node ./scripts/prepare-package.js",
    "publish": "npm publish ./lib",
    "tests": "vitest run --passWithNoTests",
    "tests:ci": "vitest run --passWithNoTests --reporter=junit --outputFile.junit=junit.xml",
    "lint:types": "tsc --noEmit",
    "lint": "eslint --fix --quiet",
    "lint:all": "eslint \"**/*.{ts,tsx}\" --fix --quiet"
  },
  "dependencies": {
    "@astral/utils": "^1.6.1",
    "@emotion/hash": "0.9.2",
    "fast-json-stable-stringify": "2.1.0",
    "utility-types": "3.11.0"
  },
  "peerDependencies": {
    "mobx": "^6.0.0"
  },
  "devDependencies": {
    "mobx": "^6.9.0",
    "tsdown": "^0.17.2",
    "vitest-mock-extended": "^1.3.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
